{
    "name": "rkaiser0324/schema-manage",
    "description": "SchemaManage plugin for CakePHP, as an alternative to the Migrations plugin",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "dbdiff/dbdiff": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SchemaManage\\": "src/"
        }
    },
    "repositories": [
        {
            "url": "https://github.com/rkaiser0324/DBDiff",
            "type": "git"
        }
    ],
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
